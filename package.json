{
  "name": "@ubundi/openclaw-cortex",
  "version": "0.3.4",
  "description": "OpenClaw plugin for Cortex memory â€” Auto-Recall, Auto-Capture, and file sync",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ubundi/openclaw-cortex.git"
  },
  "bugs": {
    "url": "https://github.com/Ubundi/openclaw-cortex/issues"
  },
  "homepage": "https://github.com/Ubundi/openclaw-cortex#readme",
  "openclaw": {
    "extensions": [
      "./dist/index.js"
    ]
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run -c vitest.integration.config.ts",
    "verify-release": "node scripts/verify-release.mjs",
    "clean": "rm -rf dist",
    "version": "node scripts/sync-version.mjs && git add openclaw.plugin.json"
  },
  "files": [
    "dist",
    "openclaw.plugin.json",
    "LICENSE"
  ],
  "keywords": [
    "openclaw",
    "openclaw-plugin",
    "cortex",
    "memory",
    "long-term-memory"
  ],
  "license": "MIT",
  "peerDependencies": {
    "openclaw": ">=0.1.0"
  },
  "dependencies": {
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "hono": "^4.11.9",
    "typescript": "^5.5.0",
